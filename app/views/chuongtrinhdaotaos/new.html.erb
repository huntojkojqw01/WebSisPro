<%=provide(:title,"Thêm dang ki hoc phan")%>
<div class="row">
	<div class="col-md-9">
		<h1>Thêm dang ki hoc phan mới </h1>
		<%= form_for(@chuongtrinhdaotao) do |f| %>
		    <%= render 'shared/error_messages', object: f.object %>
		    <%= f.label :"Lop sinh vien" %>
		    <%= f.collection_select :lopsinhvien_id, Lopsinhvien.all, :id, :tenlopsinhvien, prompt: true %>
		    <%= f.label :"Hoc phan" %>
		    <%= f.collection_select :hocphan_id, Hocphan.all, :id, :mahocphan, prompt: true %>
		    <%= f.label :"Hoc ki" %>
		    <%= f.number_field :hocki %>
		    <%= f.submit "Thêm ",class: "btn btn-primary"%>
		<% end %>
	</div>
	<div class="col-md-3">
			  	
	</div>
</div>
