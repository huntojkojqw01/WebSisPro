<table class="table table-hover table-striped table-bordered ">
			<thead>
				<tr>
				<th>STT</th>				
				<th>Mã học phần </th>
				<th>Tên học phần </th>
				<th>Hệ số </th>
				<th>Tín chỉ </th>
				<th>Tín chỉ học phí </th>
				<th>Số lớp học </th>
				</tr>
			</thead>
			<tbody>
			<% @count=1%>
			<% tc=0%>
			<% tchp=0%>
				<% hps.each do |x|%>
				<tr>
					<td><%= @count %></td>					
					<td><%=x.mahocphan%></td>
					<td><%=x.tenhocphan%></td>
					<td><%=(1-x.trongso).round(1)%>-<%=x.trongso%></td>
					<td><%=x.tinchi%></td>
					<td><%=x.tinchihocphi%></td>
					<td><%=x.lophocs.where("hocki_id=?",current_hocki.id).count%></td>
					<%tc+=x.tinchi%>
					<%tchp+=x.tinchihocphi%>					
				</tr>
				<% @count+=1 %>
				<%end%>
				<tr>
				<td></td><td></td><td></td><td></td>
				<td><%=tc%></td>
				<td><%=tchp%></td>
				</tr>
			</tbody>
</table>
