<div id="khoavien_list">
	<table class="table table-striped table-bordered">
		<thead>
			<tr>
				<th>Tên khoa viện</th><th>Địa điểm</th><th>Số điện thoại</th>
			</tr>
		</thead>
		<tbody>
			<%= render @khoaviens %>				
		</tbody>
	</table>
	<%=will_paginate @khoaviens, renderer: BootstrapPagination::Rails, :outer_window=>1, :inner_window=>1 %><br>
	<%=page_entries_info @khoaviens, model: "Khoa viện"%>
</div>
