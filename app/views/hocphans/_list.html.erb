<div id="hocphan_list">
	<table class="table table-striped table-bordered">
		<thead>
			<tr>
				<th>Mã học phần</th>
				<th>Tên học phần</th>
				<th>TC</th>
				<th>TCHP</th>
				<th>Trọng số</th>
				<th>Khoa viện</th>
			</tr>
		</thead>
		<tbody>
			<%= render @hocphans %>
		</tbody>
	</table>	
	<%= will_paginate @hocphans ,renderer: BootstrapPagination::Rails, :outer_window=>1,:inner_window=>1 %>
	<%= page_entries_info @hocphans, model: "Học phần" %><br>
</div>
