<div class="row" style="background:#0B2161;padding: 2px">
  <div class="row">
      <div class="col-sm-4" id="logo">
           <img class="img-responsive" src="http://soict.hust.edu.vn/~soict2013/images/HUST_logo.jpg" alt="Chania" width="100" height="150">
      </div>
      <div class="col-sm-8">
          <h1 style="color:#D8D8D8;text-align: left" class="text" id="sis">Student System Information</h2>
          <h3 style="color:#D8D8D8;text-align: left" class="text" id="hust">Hanoi University of Science and Technology</h3>
      </div>
  </div>
  <div class="row">  
    <div class="col-sm-4 col-sm-offset-6">
        <% time=DateTime.now.to_date %>
        <h5 class="text" style="color:white;text-align: right"><% if current_hocki %>Học kì <%=current_hocki.mahocki%>,<%end%> ngày <%=time.day%> tháng <%=time.month%> năm <%=time.year%></h5>
    </div>
  </div>
</div>
